@model List<ChineseNet_98K.Entity.ChapterHelper>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DemoPageMarginsPDF</title>
</head>
<body>
    @for (int i = 0; i < Model.Count; i++)
    {
        <br />
        <br />
        <h2>第 @(i+1) 章 @Model[i].ChapterName</h2>
        <p>@Model[i].ChapterContent.Replace("\r","\r\n")</p>
        <br />
        <br />
    }
</body>
</html>
